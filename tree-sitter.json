    "license": "MIT",
    "description": "parsing hotcrp offline review files",
    "authors": [
      {
        "name": "Wenxuan Shi",
        "email": "whexy@outlook.com",
        "url": "https://shiwx.org/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-hotcrp"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
